<div class="row">
	<div class="partialleft">
		<div class="sidebar">
			<div class="ui vertical demo menu">
				<div class="item">
					<div class="ui transparent icon input">
						<input ng-model="search"  type="text" placeholder="Search...">
						<i class="search icon"></i>
                    </div>
                </div>
			</div>
		</div>
	</div>
	<div class="9u">
	
		<div class="content content-right partialright">
    		<table class="ui celled table ">
    			<thead>
    				<tr>
    					<th>Team Name</th>
    					<th>Players</th>
    					<th>Detail</th>
    				</tr>
    			</thead>
    			<tbody>
					<tr ng-repeat="team in team_list  | filter:search">	
							<td ng-bind = "team.team_name"></td>
							<td ng-bind = "team.player_num"></td>
							<td><a ng-click="view(team.team_id)">Details</a></td>
					</tr>
				</tbody>
   			</table>
    	</div>
	</div>
</div>