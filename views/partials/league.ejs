<div class="row">
	<div class="partialleft">
		<div class="sidebar">
			<div class="ui vertical demo menu">
				<div class="item">
					<div class="ui transparent icon input">
						<input ng-model="search"  type="text" placeholder="Search...">
						<i class="search icon"></i>
                    </div>
                </div>
			</div>
		</div>
	</div>
	<div class="9u">
	
		<div class="content content-right partialright">
    		<table class="ui celled table ">
    			<thead>
    				<tr>
    					<th>Name</th>
    					<th>Teams</th>
    					<th>Year</th>
    					<th>Detail</th>
    				</tr>
    			</thead>
    			<tbody>
					<tr ng-repeat="league in leagues  | filter:search">
						<td ng-bind = "league.league_name"></td>
						<td ng-bind = "league.team_num"></td>
						<td ng-bind = "league.year"></td>
						<td><a ng-click="view(league.league_id)">Details</a></td>
					</tr>
				</tbody>
   			</table>
    	</div>
	</div>
</div>