<div class="row">
	
	<div class="3u">
		<div class="sidebar">
			<div class="ui vertical demo menu">
				<a class="item">A</a>
				<a class="item">B</a>
				<div class="item">
					<div class="ui transparent icon input">
						<input type="text" placeholder="Search...">
						<i class="search icon"></i>
                    </div>
                </div>
			</div>
		</div>
	</div>

	<div class="9u important(collapse)">
		<div class="content content-right">
			<table class="ui celled table">
				<thead>
					<tr>
						<th>Name</th>
						<th>City</th>
						<th>Year</th>
					</tr>
				</thead>
				<tbody>
					<tr ng-repeat="league in leagues">
						<td ng-bind = "league.league_name"></td>
						<td ng-bind = "league.city_id"></td>
						<td ng-bind = "league.year"></td>
					</tr>
				</tbody>
			</table>
			<div ng-repeat="league in leagues">
				<span ng-click = "view(league.league_id)" ng-bind = "league.league_name"><span>
			</div>
		</div>
	</div>
</div>